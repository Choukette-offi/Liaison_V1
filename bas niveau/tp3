.main 
.var
i
j
res 
.end-var
BIPUSH 5
ISTORE i
BIPUSH 3
ISTORE j
BIPUSH O 
ISTORE res
LOOP : ILOAD j
IFEQ END
ILOAD res
VLOAD I 
IADD
ISTORE RES
IINC j -1
GOTO LOOP
END : ILOAD res
.end-main

.main
.var
i
j
res 
.end-var
BIPUSH 6
ISTORE i
BIPUSH 3
ISTORE j
BIPUSH 0
ISTORE res
LOOP: ILOAD j
IFEQ END
BIPUSH 40
ILOAD res
ILOAD i 
ILOAD j
INVOKEVIRTUAL f
ISTORE res
IINC j -1
GOTO LOOP
END: ILOAD res
.end-main

.method f(res,i,j)
ILOAD j
IINC j -1
IFEQ END
ILOAD res
ILOAD i
IADD
ISTORE res
BIPUSH 40
ILOAD res
ILOAD i 
ILOAD j
INVOKEVIRTUAL f
ILOAD res
END: IRETURN
.end-method

.main
